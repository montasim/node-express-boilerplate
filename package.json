{
    "name": "node-express-boilerplate",
    "version": "1.0.0",
    "description": "Create a Node.js app for building production-ready RESTful APIs using Express, by running one command",
    "bin": "bin/createNodejsApp.js",
    "main": "src/server.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/montasim/node-express-boilerplate"
    },
    "bugs": {
        "url": "https://github.com/montasim/node-express-boilerplate/issues"
    },
    "homepage": "https://github.com/montasim/node-express-boilerplate#readme",
    "author": {
        "name": "Mohammad Montasim -Al- Mamun Shuvo",
        "email": "montasimmamun@gmail.com",
        "mobile": "+8801722815469",
        "portfolio": "https://montasim-dev.web.app/",
        "linkedin": "https://www.linkedin.com/in/montasim",
        "github": "https://github.com/montasim"
    },
    "license": "MIT",
    "type": "module",
    "engines": {
        "node": ">=18.x",
        "npm": ">=10.2.3"
    },
    "scripts": {
        "minify": "mkdirp dist/min && terser src/server.js --compress ecma=2015,computed_props=false -o dist/min/server.min.js",
        "obfuscate": "mkdirp dist/obf && javascript-obfuscator dist/min/server.min.js --output dist/obf/server.obf.js",
        "usethiscommandtobuild": "yarn minify && yarn obfuscate",
        "start": "pm2 start ecosystem.config.json --no-daemon",
        "dev": "cross-env NODE_ENV=development nodemon src/server.js",
        "test": "jest -i --colors --verbose --detectOpenHandles",
        "test:watch": "jest -i --watchAll",
        "coverage": "jest -i --coverage",
        "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check **/*.js",
        "prettier:fix": "prettier --write **/*.js",
        "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
        "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
        "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
        "prepare": "husky install",
        "generate-docs": "jsdoc -c jsdoc.json",
        "release": "standard-version"
    },
    "keywords": [
        "node",
        "node.js",
        "boilerplate",
        "generator",
        "express",
        "rest",
        "api",
        "mongodb",
        "mongoose",
        "es6",
        "es7",
        "es8",
        "es9",
        "jest",
        "travis",
        "docker",
        "passport",
        "joi",
        "eslint",
        "prettier"
    ],
    "dependencies": {
        "-": "^0.0.1",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "connect-mongo": "^5.1.0",
        "connect-timeout": "^1.9.0",
        "cors": "^2.8.5",
        "cross-env": "^7.0.0",
        "dotenv": "^16.4.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "expect": "^29.7.0",
        "express": "^4.19.2",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^7.2.0",
        "express-session": "^1.18.0",
        "express-useragent": "^1.0.15",
        "file-type": "^19.0.0",
        "g": "^2.0.1",
        "googleapis": "^134.0.0",
        "helmet": "^7.1.0",
        "hpp": "^0.2.3",
        "http-status": "^1.7.4",
        "joi": "^17.12.2",
        "jsdoc": "^4.0.2",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.30.1",
        "mongoose": "^8.2.2",
        "morgan": "^1.9.1",
        "multer": "^1.4.5-lts.1",
        "node-cache": "^5.1.2",
        "nodemailer": "^6.9.12",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "pm2": "^5.3.1",
        "request-ip": "^3.3.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0",
        "validator": "^13.11.0",
        "winston": "^3.12.0",
        "xss-clean": "^0.1.4"
    },
    "devDependencies": {
        "coveralls": "^3.0.7",
        "eslint": "^8.57.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^27.9.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-security": "^2.1.1",
        "faker": "^6.6.6",
        "husky": "9.0.11",
        "javascript-obfuscator": "^4.1.0",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.2",
        "mkdirp": "^3.0.1",
        "node-mocks-http": "^1.14.1",
        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "supertest": "^6.3.4",
        "terser": "^5.29.2"
    }
}
